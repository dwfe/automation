    "jpeg-js": "0.4.3",
    "node-fetch": "2.6.7",
    "pixelmatch": "5.2.1",
    "playwright": "1.9.1"
  },
  "devDependencies": {
    "@do-while-for-each/tsconfig": "^1.0.3",
    "@do-while-for-each/common": "^1.2.4",
    "@types/jpeg-js": "0.3.7",
    "@types/node-fetch": "2.5.12",
